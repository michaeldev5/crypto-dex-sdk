{
  "name": "@crypto-dex-sdk/wagmi-config",
  "version": "0.1.2",
  "description": "Zenlink Wagmi Config",
  "author": "zenlinkpro",
  "license": "LGPL-2.1-or-later",
  "keywords": [
    "zenlink-interface",
    "wagmi",
    "config"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "./index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
