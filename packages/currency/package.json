    "@ethersproject/units": "^5.7.0",
    "lodash.flatmap": "^4.5.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@crypto-dex-sdk/eslint-config": "workspace:*",
    "@crypto-dex-sdk/typescript-config": "workspace:*",
    "@types/lodash.flatmap": "^4.5.9",
    "@types/react": "^18.2.41",
