{
  "name": "price-script",
  "version": "0.0.2",
  "description": "Price script",
  "license": "LGPL-2.1-or-later",
  "keywords": [
    "zenlink-interface",
    "price",
    "script"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "start": "esno src/run.ts",
    "server": "ts-node --swc src/server.ts",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "TIMING=1 eslint src --fix"
  },
  "devDependencies": {
    "@apollo/client": "^3.8.8",
    "@crypto-dex-sdk/chain": "workspace:*",
    "@crypto-dex-sdk/currency": "workspace:*",
    "@crypto-dex-sdk/eslint-config": "workspace:*",
    "@crypto-dex-sdk/graph-client": "workspace:*",
    "@crypto-dex-sdk/graph-config": "workspace:*",
    "@crypto-dex-sdk/typescript-config": "workspace:*",
    "@types/express": "^4.17.21",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "esno": "^4.0.0",
    "express": "^4.18.2",
    "fast-json-stable-stringify": "^2.1.0",
    "graphql": "16.8.1",
    "ioredis": "^5.3.2",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "5.3.2"
  }
